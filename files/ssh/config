Host github.com
  User git
  HostName github.com
  IdentityFile ~/.ssh/github_rsa
Host bitbucket.org
  User git
  Port 22
  HostName bitbucket.org
  IdentityFile ~/.ssh/bitbucket_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes
Host gitlab.com
  User git
  HostName gitlab.com
  IdentityFile ~/.ssh/gitlab_rsa
  TCPKeepAlive yes
  IdentitiesOnly yes
